from tkinter import *
from unittest import result
from image_position import *


def interface():
    centers = getPosition()

    master = Tk()
    master.geometry("200x200")

    variable = StringVar(master)
    variable.set(centers[0]) # default value

    w = OptionMenu(master, variable, *centers)
    w.pack()

    def ok():
        result = variable.get()
        print ("value is:" + result)

    button = Button(master, text="OK", command=ok)
    button.pack()

    mainloop()
    
    return result